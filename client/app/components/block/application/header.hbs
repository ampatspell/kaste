<div class="block-application-header" {{class @class}}>
  <LinkTo class="name" @route="index">{{this.name}}</LinkTo>
  <div class="delimiter"></div>
  {{#if this.store.user}}
    <LinkTo @route="session.delete">sign out</LinkTo>
  {{else}}
    <LinkTo @route="session.new">sign in</LinkTo>
  {{/if}}
</div>